{
    "name": "looaf/laravel-erd",
    "description": "A Laravel package that automatically generates interactive entity relationship diagrams from Eloquent models",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "erd",
        "entity-relationship-diagram",
        "eloquent",
        "database",
        "visualization"
    ],
    "authors": [
        {
            "name": "Adam Caruana",
            "email": "adam@caruana.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelErd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelErd\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelErd\\ErdServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}